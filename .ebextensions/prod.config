container_commands:
    01_setup_apache:
        command: "cp .ebextensions/enable_mod_deflate.conf /etc/httpd/conf.d/enable_mod_deflate.conf"

option_settings:
  - namespace: 'aws:autoscaling:asg'
    option_name: MinSize
    value: 2
  - namespace: 'aws:autoscaling:asg'
    option_name: MaxSize
    value: 20
